{
  "name": "vite-plugin-ignore-dynamic-imports",
  "version": "1.0.2",
  "description": "A Vite plugin that automatically adds /* @vite-ignore */ comments to dynamic imports",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeYallop/WasmSharp.git",
    "directory": "packages/vite-plugin-ignore-dynamic-imports"
  },
  "bugs": {
    "url": "https://github.com/JakeYallop/WasmSharp/issues"
  },
  "homepage": "https://github.com/JakeYallop/WasmSharp/tree/main/packages/vite-plugin-ignore-dynamic-imports",
  "author": "JakeYallop",
  "license": "Apache-2.0",
  "keywords": [
    "vite",
    "plugin",
    "dynamic-imports",
    "vite-ignore",
    "transform",
    "imports",
    "import-analysis"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "pnpm build && pnpm publish"
  },
  "files": ["dist"],
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "unplugin-ast": "catalog:",
    "@babel/generator": "catalog:"
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@types/babel__generator": "catalog:",
    "cpy-cli": "catalog:",
    "unbuild": "catalog:"
  }
}
