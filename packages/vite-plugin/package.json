{
  "name": "@wasmsharp/vite-plugin",
  "version": "0.10.0",
  "description": "A plugin for using WasmSharp with vite.",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeYallop/WasmSharp.git",
    "directory": "packages/vite-plugin"
  },
  "bugs": {
    "url": "https://github.com/JakeYallop/WasmSharp/issues"
  },
  "homepage": "https://github.com/JakeYallop/WasmSharp/tree/main/packages/vite-plugin",
  "author": "JakeYallop",
  "license": "Apache-2.0",
  "keywords": [
    "vite",
    "plugin",
    "wasm",
    "webassembly",
    "wasmsharp"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch"
  },
  "dependencies": {
    "vite": "catalog:",
    "vite-plugin-ignore-dynamic-imports": "workspace:*"
  },
  "devDependencies": {
    "unbuild": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0",
    "@wasmsharp/core": "workspace:*"
  }
